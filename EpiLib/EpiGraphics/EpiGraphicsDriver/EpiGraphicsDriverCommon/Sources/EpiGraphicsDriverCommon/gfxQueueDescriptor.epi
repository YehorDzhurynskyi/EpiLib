class gfxQueueDescriptor
{
    gfxQueueType Type;

    [WriteCallback(RValueRef=true)]
    gfxQueueFamily QueueFamily;

    [ReadOnly]
    {
        epiArray<epiFloat> Priorities;

        [Virtual]
        epiU32 DesiredQueueCount;
    }
};

class gfxQueueDescriptorList
{
    [Virtual, ReadOnly]
    epiSize_t Size;
};
