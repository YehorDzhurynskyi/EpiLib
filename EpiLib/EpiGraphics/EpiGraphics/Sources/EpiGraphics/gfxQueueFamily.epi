class gfxQueueFamilyDescriptor
{
    [Virtual, ReadOnly]
    {
        gfxQueueType QueueTypeSupportedMask;
        epiU32 QueueCount;
    }
};

class gfxQueueFamily
{
    [ReadOnly]
    {
        epiArray<gfxQueue> Queues;

        [Virtual]
        {
            gfxQueueType QueueTypeSupportedMask;
            epiU32 QueueCount;
        }
    }
};
